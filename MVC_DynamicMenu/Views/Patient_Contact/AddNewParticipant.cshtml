                        @*Demographic*@

                        <button type="button" class="accordion">Demographic</button>
                        <div class="panel">
                            <div class="form-group">
                                <p>Demographic</p>
                                <label id="name-label" asp-for="Patient_Info[0].NDIS_Number">NDIS Number</label>
                                <input type="text"
                                       asp-for="Patient_Info[0].NDIS_Number"
                                       id="name"
                                       class="form-control"
                                       placeholder="--9 characters starts with 43--"
                                       required />

                           

                                <label id="name-label" asp-for="Patient_Info[0].FirstName">First Name</label>
                                <input type="text"
                                       asp-for="Patient_Info[0].FirstName"
                                       id="name"
                                       class="form-control"
                                       required />
                                <br />
                                <label id="name-label" asp-for="Patient_Info[0].LastName">Last Name</label>
                                <input type="text"
                                       asp-for="Patient_Info[0].LastName"
                                       id="name"
                                       class="form-control"
                                       required />
                                <br />
                                <label id="name-label" asp-for="Patient_Info[0].Age">Age</label>
                                <input type="number"
                                       asp-for="Patient_Info[0].Age"
                                       id="name"
                                       class="form-control"
                                    />
                                <br />
                                <h6>Gender</h6>
                                <select id="dropdown" asp-for="Patient_Info[0].Gender" class="form-control" >
                                    @*<option disabled selected value>Gendere</option>*@
                                    <option value="Male">Male</option>
                                    <option value="Female">Female</option>
                                    <option value="Other">Other</option>
                                </select>

                                <label asp-for="Patient_Info[0].DateOfBirth">Date Of Birth</label>
                                <input type="date" asp-for="Patient_Info[0].DateOfBirth" id="dateofbirth" required>

                                <h6>Office</h6>
                                <select id="dropdown" asp-for="Patient_Info[0].Office" class="form-control" required>
                                    @*<option disabled selected value>Gendere</option>*@
                                    <option value="Melbourne">Melbourne</option>
                                    <option value="Darwin">Darwin</option>
                                </select>


                            </div>
                        </div>





